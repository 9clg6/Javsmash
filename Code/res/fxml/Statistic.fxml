<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" xmlns="http://javafx.com/javafx/2.2">

    <TableView fx:id="laListe" GridPane.columnIndex="0" GridPane.rowIndex="0"/>

    <Button fx:id="LoadButton" contentDisplay="CENTER" text="Load" textAlignment="CENTER" GridPane.columnIndex="0"
            GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
        <GridPane.margin>
            <Insets bottom="10.0" top="10.0" fx:id="x1"/>
        </GridPane.margin>
    </Button>

    <Button fx:id="SaverButton" contentDisplay="CENTER" text="Save" textAlignment="CENTER" GridPane.columnIndex="0"
            GridPane.halignment="CENTER" GridPane.margin="$x1" GridPane.rowIndex="2" GridPane.valignment="CENTER"/>

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="40.0" vgrow="SOMETIMES"/>
    </rowConstraints>

</GridPane>
